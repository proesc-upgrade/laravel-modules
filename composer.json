{
    "name": "creolab/laravel-modules",
    "keywords": ["modules", "laravel"],
    "description": "Application specific modules in Laravel 6+",
    "homepage": "http://creolab.hr",
    "authors": [
        {
            "name": "Boris Strahija",
            "email": "bstrahija@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "way/generators": "~2"
    },
    "suggest": {
        "way/generators": "Required for modules:generate functionality."
    },
    "autoload": {
        "psr-4": {
            "Creolab\\LaravelModules\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
